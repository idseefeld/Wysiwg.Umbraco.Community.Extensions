{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "158f2051-3145-41a5-acd9-a62ccd40c7fc",
        "SanitizeTinyMce": true,
        "UseHttps": true,
        "Smtp": {
          "From": "umbraco@idseefeld.de",
          "Port": 25,
          "Host": "localhost"
        }
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Notifications": { "Email": "notifications@idseefeld.de" }
      },
      "Runtime": { "Mode": "Development" },
      "ModelsBuilder": { "ModelsMode": "Nothing" },
      "Unattended": {
        "UpgradeUnattended": true,
        "UnattendedUserName": "Administrator",
        "UnattendedUserEmail": "admin@example.com",
        "UnattendedUserPassword": "1234567890"
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "WebRouting": { "UmbracoApplicationUrl": "https://[MAIN-DOMAIN]/" }
    }
  }
}
